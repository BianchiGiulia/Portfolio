    "def text_based_word2vec_retrieval(query_id, w_to_vec, songs_df, N):\n",
    "    # get tf-idf vector for the query song\n",
    "    query_vector = w_to_vec[w_to_vec['id'] == query_id].iloc[:, 1:].values\n",
    "    \n",
    "    # calculate cosine similarity with all songs\n",
    "    all_songs = w_to_vec.iloc[:, 1:].values\n",
    "    similarities = cosine_similarity(query_vector, all_songs).flatten()\n",
    "    \n",
    "    # get the top N similar songs â¹ scores\n",
    "    top_indices = similarities.argsort()\n",
    "    top_indices = top_indices[-N-1:-1][::-1]    # exclude the first most similar (= query itself)\n",
    "    top_similar_songs = songs_df.iloc[top_indices]\n",
    "    similarity_scores = similarities[top_indices]\n",
    "\n",
    "    return top_similar_songs, similarity_scores"
   ]
  },
