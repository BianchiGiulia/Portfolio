    "def display_top_songs(query_id, N=10):\n",
    "    \n",
    "    random_songs = random_baseline(query_id, songs_df, N)\n",
    "    tfidf_top_songs, tf_idf_similarity_scores = text_based_retrieval(query_id, tfidf_df, songs_df, N)\n",
    "    word2vec_top_songs, w2v_similarity_scores = text_based_word2vec_retrieval(query_id, w_to_vec, songs_df, N)\n",
    "    bert_top_songs, bert_similarity_scores = text_based_bert_retrieval(query_id, bert_df, songs_df, N)\n",
    "    combined_top_songs, combined_similarity_scores = combined_retrieval(query_id, tfidf_df, w_to_vec, songs_df, N)\n",
    "    \n",
    "    # df with results to display\n",
    "    top_songs_df = pd.concat({\n",
    "        'Random Baseline': random_songs['song'].reset_index(drop=True),\n",
    "        'TF-IDF': tfidf_top_songs['song'].reset_index(drop=True),\n",
    "        'Word2Vec': word2vec_top_songs['song'].reset_index(drop=True),\n",
    "        'BERT': bert_top_songs['song'].reset_index(drop=True),\n",
    "        'Combined': combined_top_songs['song'].reset_index(drop=True)}, axis=1)\n",
    "    \n",
    "    # df with similarity scores\n",
    "    similarity_scores_df = pd.DataFrame({\n",
    "        'TF-IDF': tf_idf_similarity_scores,\n",
    "        'Word2Vec': w2v_similarity_scores,\n",
    "        'BERT': bert_similarity_scores,\n",
    "        'Combined': combined_similarity_scores\n",
    "    })\n",
    "    similarity_scores_df.to_csv(f'similarity_scores_{query_id}.csv', index=False)\n",
    "    display(top_songs_df) \n"
   ]
  },
