    "def text_based_bert_retrieval(query_id, bert_df, songs_df, N):\n",
    "    query_id = query_song['id']\n",
    "    #check if the query_id is in the bert_df dataFrame\n",
    "    if query_id not in bert_df['id'].values:\n",
    "        return []  # return an empty list if not\n",
    "\n",
    "    # extract the BERT vector for the query song\n",
    "    query_vector = bert_df.loc[bert_df['id'] == query_id].iloc[:, 1:769].values.astype(float).flatten()\n",
    "\n",
    "    # calculate cosine similarity with all songs\n",
    "    all_vectors = bert_df.iloc[:, 1:769].values.astype(float)\n",
    "    similarities = cosine_similarity([query_vector], all_vectors)[0]\n",
    "\n",
    "    # get the indices of the top N similar songs (excluding the query song itself)\n",
    "    top_indices = similarities.argsort()[:-N-1:-1]\n",
    "\n",
    "    # get the top N similar songs from the songs_df DataFrame\n",
    "    top_similar_songs = songs_df.iloc[top_indices]\n",
    "    similarity_scores = similarities[top_indices]\n",
    "\n",
    "    return top_similar_songs, similarity_scores"
   ]
  },
