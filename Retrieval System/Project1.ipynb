   "source": [
    "def text_based_retrieval(query_id, tfidf_df, songs_df, N):\n",
    "    #get the tf-idf vector without id for the query song\n",
    "    query_vector = tfidf_df[tfidf_df['id'] == query_id].iloc[:, 1:].values #shape 1x1000    \n",
    "    \n",
    "    #get the tf-idf vector without id for all song (included query song!)\n",
    "    all_songs = tfidf_df.iloc[:, 1:].values # shape 10095x1000\n",
    "   \n",
    "    similarities = cosine_similarity(query_vector, all_songs).flatten() #shape (10095,)\n",
    "    \n",
    "    #get the top N similar songs & scores\n",
    "    top_indices = similarities.argsort() #ascending order\n",
    "    top_indices = top_indices[-N-1:-1][::-1]  # take top N+1 while excluding the first most similar (query song) and revert order\n",
    "    top_similar_songs = songs_df.iloc[top_indices]\n",
    "    similarity_scores = similarities[top_indices]\n",
    "\n",
    "    return top_similar_songs, similarity_scores"
   ]
  },
